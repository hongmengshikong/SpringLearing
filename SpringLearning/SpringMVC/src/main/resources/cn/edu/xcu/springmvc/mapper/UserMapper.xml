<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.xcu.springmvc.mapper.UserMapper">
    <select id="findUserById" parameterType="long" resultType="user">
        select *
        from t_user
        where id = #{id}
    </select>
<!--    <select id="findUserById" parameterType="long" resultType="cn.edu.xcu.springmvc.entity.User"></select>-->


</mapper>
